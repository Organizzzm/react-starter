    "babel-preset-react": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.16",
    "html-webpack-plugin": "^2.30.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-loadable": "^4.0.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
