{
  "name": "interviewed-react",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      [
        "react"
      ]
    ],
    "plugins": [
      "react-hot-loader/babel",
      "dynamic-import-webpack"
